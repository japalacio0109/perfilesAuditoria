<div class="panel panel-default">
  <div class="panel-heading">Formulario de Registro</div>
  <div class="panel-body">
    <div class="form-horizontal form-label-left">
      <div class="form-group">
        <%= label_tag 'pru_desc', 'Descripción', :class => "control-label col-md-4 col-sm-4 col-xs-12"%>
        <div class="col-md-3 col-sm-3 col-xs-12">
          <p class="form-control"><%= @nuevo.pru_desc%></p>
        </div>
      </div>
      <div class="form-group">
        <%= label_tag 'pru_date', 'Fecha', :class => "control-label col-md-4 col-sm-4 col-xs-12"%>
        <div class="col-md-3 col-sm-3 col-xs-12">
          <p class="form-control"><%= @nuevo.pru_date%></p>
        </div>
      </div>
      <div class="ln_solid"></div>
      <center>
        <a href="<%= prueindex_url%>" class="btn btn-default"><i class="fa fa-long-arrow-left"></i> Volver Atrás</a>
        <a href="<%= prueedit_path(@nuevo.pru_cont)%>" class="btn btn-warning"><i class="fa fa-pencil"></i> Editar</a>
      </center>
    </div>




  </div>
</div>
